updev()
{
    sudo nginx
    /mongo/mongodb/bin/mongod --dbpath /mongo/data --logpath /mongo/logs/mongo.log --fork
#    mysql.server start                        
}

downdev()
{
    sudo nginx -s quit
    kill $(cat /mongo/data/mongod.lock)
#    mysql.server stop
}

host187()
{
    cp /usr/local/etc/nginx/nginxconfig187.conf /usr/local/etc/nginx/nginx.conf
    sudo nginx -s reload
}

host192()
{
    cp /usr/local/etc/nginx/nginxconfig192.conf /usr/local/etc/nginx/nginx.conf
    sudo nginx -s reload
}